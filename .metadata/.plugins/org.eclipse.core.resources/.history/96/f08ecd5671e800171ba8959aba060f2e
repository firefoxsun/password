package com.zxr.domain;

import javafx.beans.property.StringProperty;

/**
 * @author zxr:zhaoxinran_dlmuit@163.com
 * @version 创建时间：2017年12月21日 下午3:24:23 类说明
 */
public class User {
	private StringProperty website;
	private StringProperty urls;
	private StringProperty userName;
	private StringProperty email;
	private StringProperty password;
	private StringProperty tel;
	private StringProperty timeStamp;
	
	public User(StringProperty website, StringProperty urls, StringProperty userName, StringProperty email,
			StringProperty password, StringProperty tel, StringProperty timeStamp) {
		super();
		this.website = website;
		this.urls = urls;
		this.userName = userName;
		this.email = email;
		this.password = password;
		this.tel = tel;
		this.timeStamp = timeStamp;
	}

	
	public StringProperty websiteProPerty() {
		return website;
	}

	public final String getProperty(){
		return website.get();
	}
	public StringProperty urlsProPerty() {
		return urls;
	}

	public final String getUrls(){
		return urls.get();
	}
	public StringProperty userNameProPerty() {
		return userName;
	}

	public final String getUserName(){
		return userName.get();
	}
	
	public StringProperty EmailProPerty() {
		return email;
	}
	
	public final String getEmail(){
		return email.get();
	}
	
	public StringProperty passwordProPerty() {
		return password;
	}

	public final String getPassword(){
		return password.get();
	}
	
	public StringProperty telProPerty() {
		return tel;
	}

	public final String getTel(){
		return tel.get();
	}
	
	public StringProperty timeStampProPerty() {
		return timeStamp;
	}

	public final String getTimeStamp(){
		return timeStamp.get();
	}
	
	public User() {
		super();
	}

	

}
